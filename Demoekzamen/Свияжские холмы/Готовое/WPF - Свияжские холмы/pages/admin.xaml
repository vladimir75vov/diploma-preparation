<Page x:Class="WPF___Свияжские_холмы.pages.admin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF___Свияжские_холмы.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="admin">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="buttonCreateReport" Content="Создать отчет" Grid.Column="1" Click="buttonCreateReport_Click"/>
        <Button x:Name="buttonLoginHistory" Content="История входа" Grid.Column="2" Click="buttonLoginHistory_Click"/>
        <Button x:Name="buttonMaterialСosts" Content="Расходы материалов" Grid.Column="3" Click="buttonMaterialСosts_Click"/>
    </Grid>
</Page>
